@{
    ViewData["Title"] = "Streams";
}
<script src="~/js/TwitchEmbed.js" type="text/javascript"></script>
<script src="https://embed.twitch.tv/embed/v1.js"></script>
<div class="text-center">
    <h1 class="display-4">Bienvenue!</h1>
    <!-- Add a placeholder for the Twitch embed -->
    <div id="twitch-embed"></div>
    <!-- Create a Twitch.Embed object that will render within the "twitch-embed" root element. -->
    <script type="text/javascript">
        TwitchEmbed(@ViewBag.width, @ViewBag.height, "@ViewBag.channel");
    </script>
    <p>Liste des streamers en live et lien vers leur chaine sur le site</p>
</div>

